              {
                "name": "string",
                "modelCount": "number",
                "models": [
                  {
                    "id": "string",
                    "name": "string",
                    "inputCost": "number",
                    "outputCost": "number",
                    "contextLength": "number",
                    "maxTokens": "number",
                    "description": "string",
                    "supportedParameters": ["string"]
                  }
                ]
              }
            ]
          }
        }
      ]
    },
