{
  "componentDataSchema": {
    "PerformanceLeaderboard": {
      "endpoints": [
        {
          "url": "/api/leaderboard",
          "params": { "period": "1M | 1W | 3M | YTD" },
          "response": {
            "period": "string",
            "data": [
              {
                "id": "string",
                "name": "string",
                "provider": "string",
                "score": "number",
                "change": "number",
                "rank": "number",
                "trend": "up | down | stable",
                "color": "string",
                "lastUpdated": "ISO8601"
              }
            ],
            "lastUpdated": "ISO8601"
          }
        },
        {
          "url": "/api/performance/timeseries",
          "params": { "period": "string", "models": "array" },
          "response": {
            "period": "string",
            "models": ["string"],
            "data": [{ "timestamp": "ISO8601", "time": "string", "[modelId]": "number" }],
            "lastUpdated": "ISO8601"
          }
        },
        {
          "url": "/api/performance/stats",
          "response": {
            "activeModels": "number",
            "benchmarksPeriod": "string",
            "bestModel": "string",
            "bestScore": "number",
            "avgChange": "string",
            "totalBenchmarks": "number",
            "lastUpdated": "ISO8601"
          }
        }
      ]
    },
    "ModelList": {
      "endpoints": [
        {
          "url": "/api/models",
          "params": { "provider": "optional" },
          "response": [
            {
              "id": "string",
              "modelId": "string",
              "displayName": "string",
              "provider": "string",
              "inputCostPer1k": "string",
              "outputCostPer1k": "string",
              "maxTokens": "number",
              "contextLength": "number",
              "description": "string",
              "architecture": "object",
              "supportedParameters": ["string"],
              "isActive": "boolean",
              "metadata": "object",
              "lastSynced": "ISO8601",
              "createdAt": "ISO8601",
              "updatedAt": "ISO8601"
            }
          ]
        }
      ]
    },
    "ModelDetails": {
      "endpoints": [
        {
          "url": "/api/models/:modelId",
          "response": {
            "id": "string",
            "modelId": "string",
            "displayName": "string",
            "provider": "string",
            "inputCostPer1k": "string",
            "outputCostPer1k": "string",
            "maxTokens": "number",
            "contextLength": "number",
            "description": "string",
            "architecture": {
              "modality": "string",
              "tokenizer": "string",
              "instruct_type": "string | null"
            },
            "supportedParameters": ["string"],
            "isActive": "boolean",
            "metadata": "object",
            "lastSynced": "ISO8601",
            "createdAt": "ISO8601",
            "updatedAt": "ISO8601"
          }
        }
      ]
    },
